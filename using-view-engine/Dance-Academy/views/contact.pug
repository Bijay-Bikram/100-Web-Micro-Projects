extends base.pug
block style 
  style
    include ../static/style.css
    include ../static/styleContact.css

block scripts
  script(src='../static/index.js')

block content
  div.container
    h1 Contact us
    form(action="/contact" method="post" class="formclass")
      input(type="text",class ="inputclass" name="name" placeholder="Enter your name")
      input(type="password",class ="inputclass" name="password" placeholder="Enter your password")
      input(type="gmail",class ="inputclass" name="gmail" placeholder="Enter your gmail")
      input(type="phone",class ="inputclass" name="phone" placeholder="Enter your phone")
      input(type="address",class ="inputclass" name="address" placeholder="Enter your address")
      button(class="btn" type="submit" )
        | Submit

